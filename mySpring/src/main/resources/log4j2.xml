<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <loggers>
        <root level = "DEBUG">
            <appender-ref ref="spring6log"/>
        </root>
    </loggers>
    <appenders>
        <console name="spring6log" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%t] %-3logger{1024} - %msg%n"/>
        </console>
    </appenders>
</configuration>